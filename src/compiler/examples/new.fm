root feature UnexploredLevel xor { --name --addLocation} "Level" "encounter(x)"

feature name {} "name" "name=x"

feature addLocation xor {--id --structure} "Place" "addLocation(x)"

    feature id {} "name" "id=x"

    feature structure xor {--size --location} "Place" "addLocation(structure=x)"

        feature size {} "declName" "x"

        feature location {} "declName" "PlaceIsx"